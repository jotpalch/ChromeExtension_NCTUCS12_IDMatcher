{
  "name": "NCTUCS12_NUM_to_NAME",
  "action": {
    "default_icon": "./icon/icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 3,
  "version": "0.1",
  "description": "match num and name",
  "host_permissions":[
    "http://www.google.com/",
    "http://*/",
    "https://*/",
    "https://cdnjs.cloudflare.com/*/",
    "https://cdn.jsdelivr.net/*/"
  ],
  "permissions": [
    "activeTab",
    "search",
    "scripting"
  ],
  "icons": {
     "128": "./icon/icon_128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
